.songs-group{
  height: 200rpx;
  width: 100%;
  border-bottom: 1px solid #e4e4e4;
  padding: 30rpx 10rpx;
  box-sizing: border-box;
}
.songs-group .info-group{
  line-height: 140rpx;
  float: left;
  width: 450rpx;
}
.songs-group .thumbnail-group{
  width: 200rpx;
  height: 140rpx;
  float: right;
}
.thumbnail-group .thumbnail{
  width: 100%;
  height: 100%
}
.info-group .more-group .author{
  font-size: 10px;
  color: black;
}
.info-group .more-group .time{
  font-size: 10px;
  
}
.info-group .title{
  font-size: 14px;
  line-height: 50rpx
}